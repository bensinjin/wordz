{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "build": "yarn run clean && yarn run tsc",
    "clean": "rimraf build",
    "lint": "tslint src/**/*.tsx",
    "test": "yarn run jest --maxWorkers=4"
  },
  "dependencies": {
    "@types/ramda": "^0.25.46",
    "@types/react-router-native": "^4.2.3",
    "expo": "^31.0.2",
    "global": "^4.3.2",
    "jest-expo": "^31.0.0",
    "native-base": "^2.10.0",
    "ramda": "^0.26.1",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-modal": "^7.0.2",
    "react-router-native": "^4.3.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-native": "^0.57.25",
    "@types/react-test-renderer": "^16.0.3",
    "babel-preset-expo": "^5.0.0",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-immutable": "^5.0.1"
  },
  "jest": {
    "preset": "jest-expo",
    "setupTestFrameworkScriptFile": "<rootDir>/lib/application/__tests__/helpers/setup_tests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|expo|@expo/.*|native-base)/)"
    ],
    "testRegex": "/__tests__/[-a-zA-Z0-9_]+\\.(test|spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": true
}
